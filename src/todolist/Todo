package todolist;

public class Todo {
	
	int id;
	String name;
	String summary;
	String description;
	String author;
	
	public Todo() {
		
	}
	
	public Todo(int id, String name, String description, String summary, String author) {
		super();
		this.id = id;
		this.name = name;
		this.description = description;
		this.summary = summary;
		this.author = author;
	}
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getSummary() {
		return summary;
	}

	public void setSummary(String summary) {
		this.summary = summary;
	}

	public String getAuthor() {
		return author;
	}

	public void setAuthor(String author) {
		this.author = author;
	}
}
